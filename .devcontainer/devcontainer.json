{
    "name": "vibero-dev",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "postCreateCommand": "poetry install",
    "features": {
      "ghcr.io/devcontainers-contrib/features/poetry:2": {
        "version": "latest"
      }
    },
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-python.vscode-pylance",
          "tamasfe.even-better-toml",
          "charliermarsh.ruff"
        ]
      }
    }
  }
  